{
  "name": "Facebook Google contact sync tool",
  "version": "1.0.10",
  "icons": { "48": "img/icon-48.png",
            "128": "img/icon-128.png" },
  "description": "Gets a list of facebook and google contacts, and allows you to copy facebook contacts to google.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Click to see your contacts on Facebook and Google",
    "default_icon": "img/icon-19-off.png"
  },
  "permissions": [
    "identity",
    "tabs",
    "http://www.google.com/m8/feeds/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken",
    "https://www.facebook.com/*",
    "https://people.googleapis.com/*"
  ],
  "manifest_version": 2,
  "key": "",
  "oauth2": {
    "client_id": "384199086477-ocq8lc8r9mt1jk2bed4k9b41pejoh2do.apps.googleusercontent.com",
    "scopes": [
      "https://www.google.com/m8/feeds/",
      "https://www.googleapis.com/auth/contacts"
    ]
  }
}
